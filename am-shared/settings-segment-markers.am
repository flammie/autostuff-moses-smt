					$(BASELINE_TEST_PREFIX).$(HFSTMORPH_PLUS_INFIX).$(TRANSLATION_L1_L2) \
					$(BASELINE_TEST_PREFIX).$(HFSTMORPH_SUFFIX_INFIX).$(TRANSLATION_L1_L2)

