## Include to automake project to create baseline settings based on corpus name.

# MOSES_BASELINE_PREFIX is the prefix for the corpus filename 

.PHONY: markers markers-$(L1) markers-$(L2) markers-$(L1)-$(L2) markers-$(L2)-$(L1) \
	markers-evals markers-$(L1)-$(L2)-evals markers-$(L2)-$(L1)-evals

if CAN_MORFESS
MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX=$(MOSES_BASELINE_PREFIX).morfessor.1best.nomarkers
MOSES_MORFESSOR1BEST_PLUS_PREFIX=$(MOSES_BASELINE_PREFIX).morfessor.1best.spaceplusspace
MOSES_MORFESSOR1BEST_SUFFIX_PREFIX=$(MOSES_BASELINE_PREFIX).morfessor.1best.suffixmarkered

# L1 to L2
MOSES_MORFESSOR1BEST_NOMARKERS_L2=$(MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX).blm.$(L1)
MOSES_MORFESSOR1BEST_NOMARKERS_L1_L2=$(MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX).btm.$(L1)-$(L2)
MOSES_MORFESSOR1BEST_NOMARKERS_L1_L2_DIR=$(MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX).$(L1)-$(L2)
MOSES_MORFESSOR1BEST_PLUS_L2=$(MOSES_MORFESSOR1BEST_PLUS_PREFIX).blm.$(L1)
MOSES_MORFESSOR1BEST_PLUS_L1_L2=$(MOSES_MORFESSOR1BEST_PLUS_PREFIX).btm.$(L1)-$(L2)
MOSES_MORFESSOR1BEST_PLUS_L1_L2_DIR=$(MOSES_MORFESSOR1BEST_PLUS_PREFIX).$(L1)-$(L2)
MOSES_MORFESSOR1BEST_SUFFIX_L2=$(MOSES_MORFESSOR1BEST_SUFFIX_PREFIX).blm.$(L1)
MOSES_MORFESSOR1BEST_SUFFIX_L1_L2=$(MOSES_MORFESSOR1BEST_SUFFIX_PREFIX).btm.$(L1)-$(L2)
MOSES_MORFESSOR1BEST_SUFFIX_L1_L2_DIR=$(MOSES_MORFESSOR1BEST_SUFFIX_PREFIX).$(L1)-$(L2)

# L2 to L1
MOSES_MORFESSOR1BEST_NOMARKERS_L1=$(MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX).blm.$(L2)
MOSES_MORFESSOR1BEST_NOMARKERS_L2_L1=$(MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX).btm.$(L2)-$(L1)
MOSES_MORFESSOR1BEST_NOMARKERS_L2_L1_DIR=$(MOSES_MORFESSOR1BEST_NOMARKERS_PREFIX).$(L2)-$(L1)
MOSES_MORFESSOR1BEST_PLUS_L1=$(MOSES_MORFESSOR1BEST_PLUS_PREFIX).blm.$(L2)
MOSES_MORFESSOR1BEST_PLUS_L2_L1=$(MOSES_MORFESSOR1BEST_PLUS_PREFIX).btm.$(L2)-$(L1)
MOSES_MORFESSOR1BEST_PLUS_L2_L1_DIR=$(MOSES_MORFESSOR1BEST_PLUS_PREFIX).$(L2)-$(L1)
MOSES_MORFESSOR1BEST_SUFFIX_L1=$(MOSES_MORFESSOR1BEST_SUFFIX_PREFIX).blm.$(L2)
MOSES_MORFESSOR1BEST_SUFFIX_L2_L1=$(MOSES_MORFESSOR1BEST_SUFFIX_PREFIX).btm.$(L2)-$(L1)
MOSES_MORFESSOR1BEST_SUFFIX_L2_L1_DIR=$(MOSES_MORFESSOR1BEST_SUFFIX_PREFIX).$(L2)-$(L1)

endif

if CAN_FLATCAT


endif


if CAN_HFST
MOSES_HFST1BESTCOMP_NOMARKERS_PREFIX=$(MOSES_BASELINE_PREFIX).hfst.1best-comp.nomarkers
MOSES_HFST1BESTMORPH_NOMARKERS_PREFIX=$(MOSES_BASELINE_PREFIX).hfst.1best-morphs.nomarkers
MOSES_HFST1BESTCOMP_PLUS_PREFIX=$(MOSES_BASELINE_PREFIX).hfst.1best-comp.spaceplusspace
MOSES_HFST1BESTMORPH_PLUS_PREFIX=$(MOSES_BASELINE_PREFIX).hfst.1best-morphs.spaceplusspace
MOSES_HFST1BESTCOMP_SUFFIX_PREFIX=$(MOSES_BASELINE_PREFIX).hfst.1best-comp.suffixmarkered
MOSES_HFST1BESTMORPH_SUFFIX_PREFIX=$(MOSES_BASELINE_PREFIX).hfst.1best-morphs.suffixmarkered
# L1 to L2
MOSES_HFST1BESTCOMP_NOMARKERS_L2=$(MOSES_HFST1BESTCOMP_NOMARKERS_PREFIX).blm.$(L1)
MOSES_HFST1BESTCOMP_NOMARKERS_L1_L2=$(MOSES_HFST1BESTCOMP_NOMARKERS_PREFIX).btm.$(L1)-$(L2)
MOSES_HFST1BESTCOMP_NOMARKERS_L1_L2_DIR=$(MOSES_HFST1BESTCOMP_NOMARKERS_PREFIX).$(L1)-$(L2)
MOSES_HFST1BESTMORPH_NOMARKERS_L2=$(MOSES_HFST1BESTMORPH_NOMARKERS_PREFIX).blm.$(L1)
MOSES_HFST1BESTMORPH_NOMARKERS_L1_L2=$(MOSES_HFST1BESTMORPH_NOMARKERS_PREFIX).btm.$(L1)-$(L2)
