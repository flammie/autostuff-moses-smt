MOSES_HFST1BESTCOMP_PLUS_L1_L2=$(MOSES_HFST1BESTCOMP_PLUS_PREFIX).btm.$(L1)-$(L2)
MOSES_HFST1BESTCOMP_PLUS_L1_L2_DIR=$(MOSES_HFST1BESTCOMP_PLUS_PREFIX).$(L1)-$(L2)
MOSES_HFST1BESTMORPH_PLUS_L2=$(MOSES_HFST1BESTMORPH_PLUS_PREFIX).blm.$(L1)
MOSES_HFST1BESTMORPH_PLUS_L1_L2=$(MOSES_HFST1BESTMORPH_PLUS_PREFIX).btm.$(L1)-$(L2)
MOSES_HFST1BESTMORPH_PLUS_L1_L2_DIR=$(MOSES_HFST1BESTMORPH_PLUS_PREFIX).$(L1)-$(L2)
MOSES_HFST1BESTCOMP_SUFFIX_L2=$(MOSES_HFST1BESTCOMP_SUFFIX_PREFIX).blm.$(L1)
MOSES_HFST1BESTCOMP_SUFFIX_L1_L2=$(MOSES_HFST1BESTCOMP_SUFFIX_PREFIX).btm.$(L1)-$(L2)
MOSES_HFST1BESTCOMP_SUFFIX_L1_L2_DIR=$(MOSES_HFST1BESTCOMP_SUFFIX_PREFIX).$(L1)-$(L2)
MOSES_HFST1BESTMORPH_SUFFIX_L2=$(MOSES_HFST1BESTMORPH_SUFFIX_PREFIX).blm.$(L1)
MOSES_HFST1BESTMORPH_SUFFIX_L1_L2=$(MOSES_HFST1BESTMORPH_SUFFIX_PREFIX).btm.$(L1)-$(L2)
MOSES_HFST1BESTMORPH_SUFFIX_L1_L2_DIR=$(MOSES_HFST1BESTMORPH_SUFFIX_PREFIX).$(L1)-$(L2)

# L2 to L1
MOSES_HFST1BESTCOMP_NOMARKERS_L1=$(MOSES_HFST1BESTCOMP_NOMARKERS_PREFIX).blm.$(L2)
MOSES_HFST1BESTCOMP_NOMARKERS_L2_L1=$(MOSES_HFST1BESTCOMP_NOMARKERS_PREFIX).btm.$(L2)-$(L1)
