MOSES_HFST1BESTMORPH_NOMARKERS_L1=$(MOSES_HFST1BESTMORPH_NOMARKERS_PREFIX).blm.$(L2)
MOSES_HFST1BESTMORPH_NOMARKERS_L2_L1=$(MOSES_HFST1BESTMORPH_NOMARKERS_PREFIX).btm.$(L2)-$(L1)
MOSES_HFST1BESTMORPH_NOMARKERS_L2_L1_DIR=$(MOSES_HFST1BESTMORPH_NOMARKERS_PREFIX).$(L2)-$(L1)
MOSES_HFST1BESTCOMP_PLUS_L1=$(MOSES_HFST1BESTCOMP_PLUS_PREFIX).blm.$(L2)
MOSES_HFST1BESTCOMP_PLUS_L2_L1=$(MOSES_HFST1BESTCOMP_PLUS_PREFIX).btm.$(L2)-$(L1)
MOSES_HFST1BESTCOMP_PLUS_L2_L1_DIR=$(MOSES_HFST1BESTCOMP_PLUS_PREFIX).$(L2)-$(L1)
MOSES_HFST1BESTMORPH_SUFFIX_L1=$(MOSES_HFST1BESTMORPH_SUFFIX_PREFIX).blm.$(L2)
MOSES_HFST1BESTMORPH_SUFFIX_L2_L1=$(MOSES_HFST1BESTMORPH_SUFFIX_PREFIX).btm.$(L2)-$(L1)
MOSES_HFST1BESTMORPH_SUFFIX_L2_L1_DIR=$(MOSES_HFST1BESTMORPH_SUFFIX_PREFIX).$(L2)-$(L1)
MOSES_HFST1BESTCOMP_SUFFIX_L1=$(MOSES_HFST1BESTCOMP_SUFFIX_PREFIX).blm.$(L2)
MOSES_HFST1BESTCOMP_SUFFIX_L2_L1=$(MOSES_HFST1BESTCOMP_SUFFIX_PREFIX).btm.$(L2)-$(L1)
MOSES_HFST1BESTCOMP_SUFFIX_L2_L1_DIR=$(MOSES_HFST1BESTCOMP_SUFFIX_PREFIX).$(L2)-$(L1)
MOSES_HFST1BESTMORPH_PLUS_L1=$(MOSES_HFST1BESTMORPH_PLUS_PREFIX).blm.$(L2)
MOSES_HFST1BESTMORPH_PLUS_L2_L1=$(MOSES_HFST1BESTMORPH_PLUS_PREFIX).btm.$(L2)-$(L1)
MOSES_HFST1BESTMORPH_PLUS_L2_L1_DIR=$(MOSES_HFST1BESTMORPH_PLUS_PREFIX).$(L2)-$(L1)
endif

# Convenience variables for models
MARKER_LMS_L1=$(MOSES_MORFESSOR1BEST_NOMARKERS_L1) \
			  $(MOSES_MORFESSOR1BEST_PLUS_L1) \
			  $(MOSES_MORFESSOR1BEST_SUFFIX_L1) \
			  $(MOSES_HFST1BESTCOMP_NOMARKERS_L1) \
			  $(MOSES_HFST1BESTCOMP_PLUS_L1) \
			  $(MOSES_HFST1BESTCOMP_SUFFIX_L1) \
			  $(MOSES_HFST1BESTMORPH_NOMARKERS_L1) \
			  $(MOSES_HFST1BESTMORPH_PLUS_L1) \
			  $(MOSES_HFST1BESTMORPH_SUFFIX_L1)
MARKER_LMS_L2=$(MOSES_MORFESSOR1BEST_NOMARKERS_L2) \
			  $(MOSES_MORFESSOR1BEST_PLUS_L2) \
			  $(MOSES_MORFESSOR1BEST_SUFFIX_L2) \
			  $(MOSES_HFST1BESTCOMP_NOMARKERS_L2) \
			  $(MOSES_HFST1BESTCOMP_PLUS_L2) \
			  $(MOSES_HFST1BESTCOMP_SUFFIX_L2) \
			  $(MOSES_HFST1BESTMORPH_NOMARKERS_L2) \
			  $(MOSES_HFST1BESTMORPH_PLUS_L2) \
			  $(MOSES_HFST1BESTMORPH_SUFFIX_L2)
markers-$(L1): $(MARKER_LMS_L1)
markers-l1: markers-$(L1)
markers-$(L2): $(MARKER_LMS_L2)
markers-l2: markers-$(L2)
LANGUAGE_MODELS_L1+=$(MARKER_LMS_L1)
LANGUAGE_MODELS_L2+=$(MARKER_LMS_L2)
LANGUAGE_MODELS+=$(LANGUAGE_MODELS_L2) $(LANGUAGE_MODELS_L1)
MARKER_TMS_L1_L2=$(MOSES_MORFESSOR1BEST_NOMARKERS_L1_L2) \
				 $(MOSES_MORFESSOR1BEST_PLUS_L1_L2) \
				 $(MOSES_MORFESSOR1BEST_SUFFIX_L1_L2) \
				 $(MOSES_HFST1BESTCOMP_NOMARKERS_L1_L2) \
				 $(MOSES_HFST1BESTCOMP_PLUS_L1_L2) \
				 $(MOSES_HFST1BESTCOMP_SUFFIX_L1_L2) \
				 $(MOSES_HFST1BESTMORPHS_NOMARKERS_L1_L2) \
				 $(MOSES_HFST1BESTMORPHS_PLUS_L1_L2) \
				 $(MOSES_HFST1BESTMORPHS_SUFFIX_L1_L2)
MARKER_TMS_L2_L1=$(MOSES_MORFESSOR1BEST_NOMARKERS_L2_L1) \
				 $(MOSES_MORFESSOR1BEST_PLUS_L2_L1) \
				 $(MOSES_MORFESSOR1BEST_SUFFIX_L2_L1) \
				 $(MOSES_HFST1BESTCOMP_NOMARKERS_L2_L1) \
				 $(MOSES_HFST1BESTCOMP_PLUS_L2_L1) \
				 $(MOSES_HFST1BESTCOMP_SUFFIX_L2_L1) \
				 $(MOSES_HFST1BESTMORPHS_NOMARKERS_L2_L1) \
				 $(MOSES_HFST1BESTMORPHS_PLUS_L2_L1) \
				 $(MOSES_HFST1BESTMORPHS_SUFFIX_L2_L1)
markers-$(L1)-$(L2): $(MARKER_TMS_L1_L2)
markers-l1-l2: markers-$(L1)-$(L2)
markers-$(L2)-$(L1): $(MARKER_TMS_L2_L1)
markers-l2-l1: markers-$(L2)-$(L1)
TRANSLATION_MODELS_L1_L2+=$(MARKER_TMS_L1_L2)
TRANSLATION_MODELS_L2_L1+=$(MARKER_TMS_L2_L1)
TRANSLATION_MODELS+=$(TRANSLATION_MODELS_L1_L2) $(TRANSLATION_MODELS_L2_L1)
MODELS+=$(TRUECASE_MODELS) $(LANGUAGE_MODELS) $(TRANSLATION_MODELS)
markers: markers-$(L1)-$(L2) markers-$(L2)-$(L1)

# translations
# baseline
if CAN_MORFESS
MORFESSOR_NOMARKERS_INFIX=morfessor.nomarkers
MORFESSOR_PLUS_INFIX=morfessor.plusspaceplus
MORFESSOR_SUFFIX_INFIX=morfessor.suffixmarkered
TRANSLATIONS_L1_L2+=$(BASELINE_TEST_PREFIX).$(MORFESSOR_NOMARKERS_INFIX).$(TRANSLATION_L1_L2) \
					$(BASELINE_TEST_PREFIX).$(MORFESSOR_PLUS_INFIX).$(TRANSLATION_L1_L2) \
					$(BASELINE_TEST_PREFIX).$(MORFESSOR_SUFFIX_INFIX).$(TRANSLATION_L1_L2)
